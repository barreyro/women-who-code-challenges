function checkInt(str){
  var strVerified = false;
  var arr = str.split('');
  for (var index = 0; index < arr.length; index++){
    var charCode = arr[index].charCodeAt();
    var cc = parseInt(charCode);
    if ( ( cc > 57) && ( cc < 48) ){
      strVerified = true;
      console.log(cc);
      break;
    } 
  }
  //var response = (strVerified === false) ? parseInt(str) : "Bad String";
  return response;
}
